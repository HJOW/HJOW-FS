{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".project":true,
        ".classpath":true
    },
    "svn.ignoreMissingSvnWarning": true,
    "java.project.sourcePaths": ["src"],
    "java.project.outputPath": "build",
    "java.project.referencedLibraries": [
        "lib/**/*.jar",
        "WebContent/WEB-INF/lib/*.jar"
    ]
}